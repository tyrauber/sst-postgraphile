{
  "name": "SLS",
  "version": "1.0.0",
  "description": "Vue/GraphQL CMS",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "test": "jest --detectOpenHandles --forceExit",
    "start": "STAGE=local NODE_ENV=development node -r esm server/index.js",
    "offline": "NODE_ENV=development sls offline",
    "deploy": "NODE_ENV=production sls deploy"
  },
  "author": "Ty Rauber <tyrauber@mac.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github.com/tyrauber/sls"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "keywords": [
    "REST",
    "GraphQL",
    "API"
  ],
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "dependencies": {
