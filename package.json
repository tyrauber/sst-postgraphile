    "koa-static": "^5.0.0",
    "koa-static-cache": "^5.1.3",
    "path-to-regexp": "^6.1.0",
    "pg": "^8.0.3",
    "postgraphile": "^4.7.0",
    "postgraphile-plugin-connection-filter": "^2.0.0",
    "secure-ls": "^1.2.6",
    "serverless-apigwy-binary": "^1.0.0",
